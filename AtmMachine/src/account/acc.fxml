                <Font name="Arial Black" size="12.0" />
            </font>
        </Button>
        <Button layoutX="280.0" layoutY="130.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="212.0" text="TRANSFER" textFill="#ddddd2" GridPane.rowIndex="2">
