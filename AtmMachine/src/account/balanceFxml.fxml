<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="/AtmLogin/loginStyle.css" xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="account.AccountDetailsController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="45.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Text fx:id="textBalance" strokeType="OUTSIDE" strokeWidth="0.0"
                  style="-fx-font-weight: bold; -fx-effect: dropshadow(gaussian, rgba(255, 255, 255, 0.5), 0,0,0,1);"
                  textAlignment="CENTER" wrappingWidth="216.13671875">
               <font>
                  <Font size="24.0" />
               </font>
            </Text>
            <HBox alignment="CENTER" prefWidth="600.0">
               <children>
                  <Button fx:id="okButton" mnemonicParsing="false" onAction="#oKAndBackButtonPressed" prefWidth="80.0" text="OK" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
